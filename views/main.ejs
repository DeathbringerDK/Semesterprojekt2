<!doctype html>

<html lang="en">

<head>
    <meta charset="utf-8">

    <title>The HTML5 Herald</title>
    <meta name="description" content="The HTML5 Herald">
    <meta name="author" content="SitePoint">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>

    <script src="js/lib/jquery.min.js"></script>
    <script src="js/infobox.js"></script>
    <link rel="stylesheet" href="css/mainstyle.css">
</head>

<body>
    <div class='topbar'>
        <div class="dropdown">
            <button class="dropbtn">Menu</button>
            <div class="dropdown-content">
                <a href="#">Option 1</a>
                <a href="#">Option 2</a>
                <a href="#">Option 3</a>
                <a href='#'>Option 4</a>
            </div>
        </div>
    </div>
    <div class='body'>
        <div id='calendarHolder'>
            <iframe src="/html/calendar.html" width="100%" height="100%"></iframe>
        </div>
        <!--Infobox med admin rettigheder.-->
        <div class='infobox'>
            <button class="createNews" onclick="createNews();">+</button>
            <div class="newsTab">
                <button class="deleteNews">-</button>
                <button class="editNews">...</button>
                <h1 class="news" contenteditable="false">Overskrift</h1>
                <p class="news" contenteditable="false">Tekst...</p>
            </div>
            <h1>TEST</h1>
            <ul>
                <%users.each(function(user) { %>
                    <li>
                        <%=user.username%> -
                            <%=user.email%>
                    </li>
                    <%});%>
            </ul>
        </div>
    </div>
    <div class='bottombar'>
        <p>This is the bottombar</p>
    </div>

</body>

</html>
